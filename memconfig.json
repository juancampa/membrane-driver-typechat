{
  "schema": {
    "types": [
      {
        "name": "Root",
        "actions": [
          {
            "description": "Configures the OpenAI API key.",
            "name": "configure",
            "type": "Void",
            "params": [
              {
                "name": "key",
                "type": "String",
                "description": "OpenAI API key."
              }
            ]
          }
        ],
        "fields": [
          {
            "name": "status",
            "type": "String"
          },
          {
            "name": "translator",
            "type": "Translator",
            "params": [
              {
                "name": "id",
                "type": "String"
              }
            ]
          }
        ],
        "events": [
          {
            "name": "statusChanged",
            "type": "Void"
          }
        ]
      },
      {
        "name": "Translator",
        "actions": [
          {
            "name": "translate",
            "type": "Json",
            "description": "Translates a prompt into a JSON value that matches the provided TypeScript schema.",
            "params": [
              {
                "name": "model",
                "type": "String",
                "description": "OpenAI model to use (e.g. gpt-3.5-turbo, gpt-4)."
              },
              {
                "name": "schema",
                "type": "String",
                "description": "A string containing the TypeScript source code for the JSON schema. The target type must be exported."
              },
              {
                "name": "typeName",
                "type": "String",
                "description": "The name of the JSON target type in the schema."
              },
              {
                "name": "prompt",
                "type": "String",
                "description": "The prompt to translate."
              }
            ]
          }
        ]
      }
    ]
  },
  "dependencies": {
    "http": "http:"
  }
}
